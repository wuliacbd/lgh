.ykHead {
  height: 15vh;
  background: #fff;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.ykHead .pic {
  width: 10vh;
  height: 10vh;
  border-radius: 50%;
  overflow: hidden;
  background: #ccc;
}

.ykHead .pic image {
  width: 100%;
  height: 100%;
}

.ykHead .username {
  font-size: 40rpx;
  padding-left: 20rpx;
  color: #333
}

.ykMain {
  height: 85vh;
}

.ykMain .noLogin {
  height: 100%;
  display: flex;
  align-items: center;
}

.ykCon {
  padding-top: 30rpx;
}

.ykRow {
  background: #fff;
  line-height: 80rpx;
  border-bottom: 1px solid #f1f1f1;
  padding: 0 30rpx;
}